import java.util.Scanner;
public class Dever {
    public static void main(String[]args){
        Scanner tcl = new Scanner(System.in);
    System.out.println("Digite a distância total");
    int a = tcl.nextInt();
    System.out.println("Digite o consumo total de combustível");
    int b = tcl.nextInt();
    int CM = (a/b);
    
    System.out.println(("O consumo médio de seu veículo é de ") + CM + (" litros por km"));
    }
}
